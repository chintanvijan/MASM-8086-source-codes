DATA SEGMENT
	A DW 0FFFFH
	MSG1 DB 10,13,'PALINDROME!$'
	MSG2 DB 10,13,'NOT PALINDROME!$'
DATA ENDS

CODE SEGMENT
	ASSUME CS:CODE,DS:DATA
	START:
		MOV AX,DATA
		MOV DS,AX
		MOV AX,A
		MOV CL,10H
	UP:	ROR AX,01H
		RCL DX,01H
		LOOP UP
		CMP AX,DX
		JNZ DW
		LEA DX,MSG1
		MOV AH,09H
		INT 21H
		JMP EX
	DW:	LEA DX,MSG2
		MOV AH,09H
		INT 21H
	EX:	MOV AH,4CH
		INT 21H
CODE ENDS
END START
