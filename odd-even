DATA SEGMENT
	NUM DB 65H
	EVE DB 0AH,0DH,"GIVEN NUMBER IS EVEN$"
	ODD DB 0AH,0DH,"GIVEN NUMBER IS ODD$"
DATA ENDS
CODE SEGMENT
	ASSUME CS:CODE,DS:DATA
	START:
		MOV AX,DATA
		MOV DS,AX

		MOV AX,NUM
		ROR AX,01H
		JC THERE
		MOV DX,OFFSET EVE
		MOV AH,09H
		INT 21H
		JMP EXIT

	THERE:	
		MOV DX,OFFSET ODD
		MOV AH,09H
		INT 21H

	EXIT:	
		INT 3

CODE ENDS
END START
