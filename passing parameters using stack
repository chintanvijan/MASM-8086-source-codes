DATA SEGMENT
	A DB 07H
	B DB 12H
	C DB ?
DATA ENDS

STACK_SEG SEGMENT
		DB 4 DUP(0)
		TOP_STACK LABEL WORD
STACK_SEG ENDS

CODE SEGMENT

	ASSUME CS:CODE,DS:DATA
	START:

		MOV AX,DATA
		MOV DS,AX

		MOV AX, STACK_SEG
		MOV SS, AX
		MOV SP, OFFSET  TOP_STACK
		
		MOV AL,A
		MOV BL,B

		PUSH AX
		PUSH BX
		CALL DO		
		MOV C,AL
		

		MOV AH,4CH
		INT 21H

		DO PROC NEAR
		MOV BP,SP
		MOV AX,[BP+1]
		MOV BX,[BP]
		ADD AX,BX		
		RET

CODE ENDS
END START
