DATA SEGMENT
	LIST DB 10 DUP(?)
DATA ENDS
CODE SEGMENT
	ASSUME CS:CODE,DS:DATA
	START:
		MOV AX,DATA
		MOV DS,AX
		MOV SI,OFFSET LIST
		MOV CX,10
		MOV AL,0
		MOV [SI],AL
		ADD AL,1
		INC SI
		MOV [SI],AL
		MOV AL,0
	BACK:	
		ADD AL,[SI]
		INC SI
		MOV [SI],AL
		DEC SI
		MOV AL,[SI]
		INC SI
		LOOP BACK
		MOV AH,4CH
		INT 21H

CODE ENDS
END START
