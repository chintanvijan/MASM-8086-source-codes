DATA SEGMENT
	STR1 DB 09H, "MAHARAJA",'$' 
	STR2 DB ?                
	ST1 DB 09H, "STR1:$"     
	ST2 DB 09H, "STR2:$"    
	LEN DB 0AH  
DATA ENDS

CODE SEGMENT
	ASSUME CS:CODE,DS:DATA
	START:
		MOV AX,DATA
		MOV DS,AX
		MOV ES,AX
		LEA SI,STR1               
		LEA DI,STR2
 
		LEA DX,ST1
		MOV AH,09H
		INT 21H

		LEA DX,STR1
		MOV AH,09H
		INT 21H

		LEA DX,ST2
		MOV AH,09H
		INT 21H
    CLD
		MOV CH,00H
		MOV CL,LEN
		REP MOVSB

		LEA DX,STR2
		MOV AH,09H
		INT 21H

		MOV AH,4CH
		INT 21H

CODE ENDS
END START



